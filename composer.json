{
    "name": "chris-ware/nova-breadcrumbs",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": ">=2.0.0"
    },
    "autoload": {
        "psr-4": {
            "ChrisWare\\NovaBreadcrumbs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisWare\\NovaBreadcrumbs\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
